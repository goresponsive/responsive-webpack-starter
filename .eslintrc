{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "es6": true,
  },
  "globals": {
    "_": false,
    "jQuery": false,
    "$": false,
    "poly": false,
    "__webpack_public_path__": false,
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
  },
  "ecmaFeatures": {
    "modules": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": false,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "spread": true,
    "restParams": true,
    "superInFunctions": true,
    "templateStrings": true,
  },
  "rules": {
    "accessor-pairs": [2, {"getWithoutSet": false, "setWithoutGet": true}],
    "array-bracket-spacing": [1, "always", {"singleValue": true, "objectsInArrays": false, "arraysInArrays": false}],
    "arrow-parens": [2, "always"],
    "arrow-spacing": [2, {"before": true, "after": true}],
    "block-scoped-var": 2,
    "block-spacing": [1, "always"],
    "brace-style": [1, "1tbs", {"allowSingleLine": false}],
    "callback-return": 0,
    "camelcase": [1, {"properties": "always"}],
    "comma-dangle": [2, "never"],
    "comma-spacing": [1, {"before": false, "after": true}],
    "comma-style": [1, "last"],
    "complexity": 0,
    "computed-property-spacing": [1, "never"],
    "consistent-return": 0,
    "consistent-this": [1, "self"],
    "constructor-super": 2,
    "curly": [2, "all"],
    "default-case": 2,
    "dot-location": [2, "property"],
    "dot-notation": [2, {"allowKeywords": true, "allowPattern": ""}],
    "eol-last": 1,
    "eqeqeq": 2,
    "func-names": 0,
    "func-style": 0,
    "generator-star-spacing": [2, {"before": false, "after": true}],
    "global-require": 0,
    "guard-for-in": 2,
    "handle-callback-err": 0,
    "id-length": 0,
    "id-match": 0,
    "indent": [1, 2, {"SwitchCase": 1, "VariableDeclarator": 1}],
    "init-declarations": 0,
    "jsx-quotes": [1, "prefer-double"],
    "key-spacing": [1, {"beforeColon": false, "afterColon": true, "mode": "minimum"}],
    "linebreak-style": 0,
    "lines-around-comment": 0,
    "max-depth": 0,
    "max-len": 0,
    "max-nested-callbacks": [1, 3],
    "max-params": 0,
    "max-statements": 0,
    "new-cap": [1, {"newIsCap": true, "capIsNew": true}],
    "new-parens": 1,
    "newline-after-var": [0, "always"],
    "no-alert": 1,
    "no-array-constructor": 1,
    "no-bitwise": 1,
    "no-caller": 2,
    "no-catch-shadow": 0,
    "no-class-assign": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-console": [1, { "allow": ["info", "warn", "error"] }],
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-continue": 1,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 1,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 0,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [1, "functions"],
    "no-extra-semi": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-func-assign": 2,
    "no-implicit-coercion": [1, {"boolean": false, "number": true, "string": false}],
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-inner-declarations": [2, "both"],
    "no-invalid-regexp": 2,
    "no-invalid-this": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 1,
    "no-loop-func": 2,
    "no-mixed-requires": 0,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": [2, {"exceptions": {"VariableDeclarator": false,  "ImportDeclaration": true, "AssignmentExpression": true, "ObjectExpression": true}}],
    "no-multi-str": 2,
    "no-multiple-empty-lines": [1, {"max": 1}],
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 1,
    "no-new-func": 2,
    "no-new-object": 1,
    "no-new-require": 0,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-obj-calls": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-path-concat": 0,
    "no-plusplus": 0,
    "no-process-env": 1,
    "no-process-exit": 0,
    "no-proto": 2,
    "no-redeclare": [2, {"builtinGlobals": true}],
    "no-regex-spaces": 2,
    "no-restricted-modules": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 0,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": [2, {"builtinGlobals": false, "hoist": "all"}],
    "no-spaced-func": 1,
    "no-sparse-arrays": 2,
    "no-sync": 0,
    "no-ternary": 0,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": [1, {"skipBlankLines": false}],
    "no-undef-init": 1,
    "no-undef": 2,
    "no-undefined": 0,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 0,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [1, {"vars": "all", "args": "none"}],
    "no-use-before-define": 1,
    "no-useless-call": 0,
    "no-useless-concat": 2,
    "no-var": 1,
    "no-void": 2,
    "no-warning-comments": [1, {"terms": ["todo", "fixme"], "location": "start"}],
    "no-with": 2,
    "object-curly-spacing": [1, "always", {"objectsInObjects": true, "arraysInObjects": true}],
    "object-shorthand": [1, "always"],
    "one-var": [1, {"uninitialized": "never", "initialized": "never"}],
    "operator-assignment": 0,
    "operator-linebreak": [1, "before"],
    "padded-blocks": [1, "never"],
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-reflect": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "quote-props": [1, "as-needed", {"keywords": false, "unnecessary": false, "numbers": false}],
    "quotes": [1, "single", "avoid-escape"],
    "radix": 1,
    "require-jsdoc": 0,
    "require-yield": 2,
    "semi-spacing": [1, {"before": false, "after": true}],
    "semi": [1, "always"],
    "sort-vars": 0,
    "keyword-spacing": [2, {"before": true, "after": true, "overrides": {"return": { "after" : true , "before" : true}}}],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "never"],
    "space-in-parens": [1, "never"],
    "space-infix-ops": [1, {"int32Hint": false}],
    "space-unary-ops": [1, {"words": true, "nonwords": false}],
    "spaced-comment": [1, "always", {"exceptions": ["/"]}],
    "strict": [0, "global"],
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "vars-on-top": 1,
    "wrap-iife": [2, "inside"],
    "wrap-regex": 1,
    "yoda": [1, "never"],
  }
}
